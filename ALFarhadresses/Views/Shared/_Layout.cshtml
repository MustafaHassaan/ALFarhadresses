<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/Styles.Css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <title>Index Al-Farha</title>
    <title>@ViewData["Title"]Index Al-Farha</title>
    <link rel="icon" href="/Dresse.ico">
    <script src="~/lib/jquery/dist/jquery.js"></script>

</head>
<body>
    <!--  navbar navbar-dark bg-dark -->
    <!-- Nave Par Pages-->
    <nav class="navbar sticky-top navbar-expand-lg navbar-dark bg-dark" style="margin-top: -40px">
        <img src="~/Logo.png">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">AL Farha Dresses</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">

            <ul class="navbar-nav mr-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Categories
                    </a>
                    <partial name="_CatList" />
                </li>
                @if (!string.IsNullOrEmpty(ViewBag.userFname))
                {
                    <li class="nav-item">
                        <a class="nav-link" asp-action="Cart" asp-controller="Cart" asp-area="Shopping">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-shopping-cart" style="margin-left: 5px;"><circle cx="9" cy="21" r="1"></circle><circle cx="20" cy="21" r="1"></circle><path d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6"></path></svg>
                            Cart
                        </a>
                    </li>
                }
                @if (string.IsNullOrEmpty(ViewBag.userFname))
                {
                    <li class="nav-item">
                        <a class="nav-link" asp-area="Role" asp-controller="Account" asp-action="Signup">

                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-users" style="margin-left: 5px;"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
                            Sign Up
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="Role" asp-controller="Account" asp-action="Login">

                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-log-in" style="margin-left: 5px;"><path d="M15 3h4a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2h-4"></path><polyline points="10 17 15 12 10 7"></polyline><line x1="15" y1="12" x2="3" y2="12"></line></svg>
                            Login
                        </a>
                    </li>
                }
                @if (!string.IsNullOrEmpty(ViewBag.userFname))
                {
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" asp-controller="Home" asp-action="Index" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Welcom @ViewBag.userFname
                        </a>
                        <div class="dropdown-menu dark bg-dark">
                            <a class="dropdown-item" asp-area="Role" asp-action="Profile" asp-controller="Account">
                                Profile
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user" style="margin-left: 33px;"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"> </path><circle cx="12" cy="7" r="4"></circle></svg>
                            </a>
                            <a class="dropdown-item" asp-area="Shopping" asp-action="Purchased" asp-controller="Index">
                                <form asp-area="Shopping" asp-controller="Purchased" asp-action="Index" method="get" class="form-inline my-2 my-lg-0" style="margin-left:-13px;">
                                    <input class="form-control mr-sm-2" name="Purchased" type="hidden" placeholder="Search" aria-label="Search" value="@ViewBag.userEmail">
                                    <button class="btn btn-primary my-2 my-sm-0" type="submit" style="color:gray;
                                            background:none;border:none;"
                                            onMouseOver="this.style.color='#FFF'"
                                            onMouseOut="this.style.color='#808080'">
                                        Purchased
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-shopping-bag" style="margin-left: 7px;">
                                            <path d="M6 2L3 6v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2V6l-3-4z"></path>
                                            <line x1="3" y1="6" x2="21" y2="6"></line>
                                            <path d="M16 10a4 4 0 0 1-8 0"></path>
                                        </svg>
                                    </button>
                                </form>
                            </a>
                            <a class="dropdown-item" asp-area="Role" asp-controller="Account" asp-action="Logout">
                                Log Out
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-log-out" style="margin-left: 23px;"><path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path><polyline points="16 17 21 12 16 7"></polyline><line x1="21" y1="12" x2="9" y2="12"></line></svg>
                            </a>
                        </div>
                    </li>
                }
            </ul>
            <form asp-area="Shopping" asp-controller="Home" asp-action="Search" method="get" class="form-inline my-2 my-lg-0">
                <input class="form-control mr-sm-2" name="Search" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-primary my-2 my-sm-0" type="submit">Search</button>
            </form>
        </div>
    </nav>
    @RenderBody()
    </div>
    <div class="card-footer text-muted dark bg-dark" style="margin-bottom: 0px;">
        <center>
            <form>
                <div class="Footer-Social" style="margin-left:-1050px;">
                    <span>
                        <a class="Footer-Social" asp-area="Shopping" asp-controller="Contactus" asp-action="Create" style=" text-decoration: none;">
                            Contact Us
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-clipboard"><path d="M16 4h2a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2h2"></path><rect x="8" y="2" width="8" height="4" rx="1" ry="1"></rect></svg>
                        </a>
                    </span>
                    <span style="margin-left: 10px;">
                        <a class="Footer-Social" href="#" style=" text-decoration: none;" data-toggle="modal" data-target="#exampleModalLong">
                            About
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-bookmark"><path d="M19 21l-7-5-7 5V5a2 2 0 0 1 2-2h10a2 2 0 0 1 2 2z"></path></svg>
                            <!-- Modal -->
                            <div class="modal fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header bg-dark">
                                            <h5 class="modal-title" id="exampleModalLongTitle" style="color: red">About</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div style="color: black;">
